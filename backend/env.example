# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Google Ads API Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=your_google_redirect_uri_here
GOOGLE_ACCESS_TOKEN=your_google_access_token_here
GOOGLE_REFRESH_TOKEN=your_google_refresh_token_here
GOOGLE_TOKEN_EXPIRY=your_google_token_expiry_here
GOOGLE_CUSTOMER_ID=your_google_customer_id_here

# JWT Secret for Authentication
JWT_SECRET=your_jwt_secret_key_here

# Auto-blocking Configuration
AUTO_BLOCK_SUSPICIOUS=false

# Environment
NODE_ENV=production

# Port (Vercel will set this automatically)
PORT=3000 